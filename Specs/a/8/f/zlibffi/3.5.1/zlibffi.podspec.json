{
  "name": "zlibffi",
  "version": "3.5.1",
  "summary": "Precompiled libffi 3.5.1 for iOS.",
  "description": "libffi 3.5.1 built for iOS as a static library, packaged for Swift/ObjC usage.",
  "homepage": "https://github.com/vforkliu/pod-libffi",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "JUN LIU": "vforkliu@gmail.com"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/vforkliu/pod-libffi.git",
    "tag": "3.5.1"
  },
  "source_files": "include/**/*.h",
  "public_header_files": "include/**/*.h",
  "preserve_paths": "lib/libffi.a",
  "vendored_libraries": "lib/libffi.a",
  "header_mappings_dir": "include",
  "requires_arc": false,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
    "SWIFT_OPTIMIZATION_LEVEL": "-Onone"
  }
}
