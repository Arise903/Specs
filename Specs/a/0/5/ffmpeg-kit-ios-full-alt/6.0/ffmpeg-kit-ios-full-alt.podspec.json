{
  "name": "ffmpeg-kit-ios-full-alt",
  "version": "6.0",
  "summary": "FFmpeg Kit iOS Full GPL Shared Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, gmp, gnutls, kvazaar, lame, libass, libilbc, libtheora, libvid.stab, libvorbis, libvpx, libwebp, zimg, libxml2, opencore-amr, opus, shine, snappy, soxr, speex, twolame, vo-amrwbenc, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "license": {
    "type": "LGPL-3.0",
    "file": "xcframeworks/ffmpegkit.xcframework/ios-arm64/ffmpegkit.framework/LICENSE"
  },
  "authors": {
    "Arthenica": "open-source@arthenica.com"
  },
  "source": {
    "git": "https://github.com/cocoapods-alt/ffmpeg-kit-ios-full-alt.git",
    "tag": "6.0"
  },
  "preserve_paths": "xcframeworks/**/*",
  "platforms": {
    "ios": "15.0"
  },
  "vendored_frameworks": [
    "xcframeworks/ffmpegkit.xcframework",
    "xcframeworks/libavcodec.xcframework",
    "xcframeworks/libavdevice.xcframework",
    "xcframeworks/libavfilter.xcframework",
    "xcframeworks/libavformat.xcframework",
    "xcframeworks/libavutil.xcframework",
    "xcframeworks/libswresample.xcframework",
    "xcframeworks/libswscale.xcframework"
  ],
  "frameworks": [
    "AudioToolbox",
    "AVFoundation",
    "CoreMedia",
    "VideoToolbox"
  ],
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ENABLE_BITCODE": "NO",
    "VALID_ARCHS": "arm64 x86_64",
    "DEFINES_MODULE": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ENABLE_BITCODE": "NO"
  }
}
