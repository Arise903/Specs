{
  "name": "IndoorsSDK",
  "version": "3.6.5",
  "summary": "IndoorsSDK - SDK для внутренней навигации с поддержкой карт, AR, и MapLibre.",
  "description": "IndoorsSDK предоставляет функционал для внутренней навигации, включая поддержку карт и AR,\nс использованием MapLibre GL для отображения карт.",
  "homepage": "https://gitlab.indoorsnavi.pro/sdk/ios-pod",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Indoors Navigation": "developer@indoorsnavi.pro"
  },
  "source": {
    "git": "https://gitlab.indoorsnavi.pro/sdk/ios-pod.git",
    "tag": "3.6.5"
  },
  "resources": [
    "IndoorsSDK.xcframework/ios-arm64/IndoorsSDK.framework/Assets/**/*",
    "IndoorsSDK.xcframework/ios-x86_64-simulator/IndoorsSDK.framework/Assets/**/*",
    "IndoorsSDK.xcframework/ios-arm64/IndoorsSDK.framework/**/*.nib",
    "IndoorsSDK.xcframework/ios-x86_64-simulator/IndoorsSDK.framework/**/*.nib",
    "IndoorsSDK.xcframework/ios-arm64/IndoorsSDK.framework/Modules/**/*.nib",
    "IndoorsSDK.xcframework/ios-x86_64-simulator/IndoorsSDK.framework/Modules/**/*",
    "IndoorsSDK.xcframework/ios-arm64/IndoorsSDK.framework/default.metallib",
    "IndoorsSDK.xcframework/ios-x86_64-simulator/IndoorsSDK.framework/default.metallib"
  ],
  "swift_versions": "5.0",
  "frameworks": [
    "MapKit",
    "CoreGraphics",
    "CoreLocation",
    "ARKit"
  ],
  "dependencies": {
    "MapLibre": [
      "~> 5.13.0"
    ]
  },
  "platforms": {
    "ios": "12.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
