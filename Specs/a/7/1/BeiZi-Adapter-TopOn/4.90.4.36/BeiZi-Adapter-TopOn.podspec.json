{
  "name": "BeiZi-Adapter-TopOn",
  "version": "4.90.4.36",
  "summary": "BeiZiSDK 适配器, 用于在TopOn聚合SDK请求广告",
  "description": "支持开屏, 原生, 插屏, 激励视频类型广告请求。",
  "homepage": "https://gitee.com/adscope/beizi-topon-adapter.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lvjunxue": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope/beizi-topon-adapter.git",
    "tag": "4.90.4.36"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "Adapter/*.{h,m}",
  "libraries": [
    "z",
    "c++",
    "c"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "dependencies": {
    "AnyThinkiOS": [
      "~>6.4.28"
    ],
    "BeiZiSDK-iOS/BeiZiSDK-iOS": [
      "~>4.90.4.36"
    ]
  },
  "xcconfig": {
    "VALID_ARCHS": "x86_64 arm64",
    "OTHER_LDFLAGS": "-ObjC"
  }
}
