{
  "name": "InmobiAdapter",
  "version": "0.0.107",
  "summary": "an adapter for Inmobi SDK",
  "description": "an adapter for Inmobi SDK for MSP C2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/MetaAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "http": "https://github.com/ParticleMedia/msp-ios-sdk-public/releases/download/0.0.107/InmobiAdapter-0.0.107.zip",
    "type": "zip"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "InmobiAdapter/InmobiAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "InMobiSDK": [
      "10.8.0"
    ],
    "MSPSharedLibraries": [
      "0.0.107"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
