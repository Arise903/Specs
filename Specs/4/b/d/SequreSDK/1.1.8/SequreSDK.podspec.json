{
  "name": "SequreSDK",
  "version": "1.1.8",
  "summary": "Sequre Scanner SDK framework.",
  "description": "Sequre Scanner SDK framework for Sequre app to detect qr code",
  "homepage": "https://github.com/IvanReyhan22/sequre-ios-sdk-test",
  "license": "",
  "authors": {
    "Ahmad Ivan Reyhan": "ivanreyhan2002@gmail.com"
  },
  "platforms": {
    "ios": "15.0"
  },
  "source": {
    "git": "https://github.com/IvanReyhan22/sequre-ios-sdk-test.git",
    "tag": "1.1.8"
  },
  "source_files": [
    "SequreSDK/**/*.{swift}",
    "SequreSDK/**/*.{h,m,mm}"
  ],
  "public_header_files": [
    "SequreSDK/**/*.h"
  ],
  "resource_bundles": {
    "SequreSDKAssets": [
      "SequreSDK/**/*.xcassets",
      "SequreSDK/**/*.gif",
      "SequreSDK/**/*.xcstrings"
    ]
  },
  "resources": [
    "SequreSDK/**/*.tflite"
  ],
  "dependencies": {
    "TensorFlowLiteTaskVision": [

    ],
    "OpenCV": [
      "4.3.0"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "ONLY_ACTIVE_ARCH": "NO",
    "VALID_ARCHS": "arm64 x86_64 armv7 armv7s arm64e",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++",
    "OTHER_LDFLAGS": "-lc++"
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "AVFoundation"
  ],
  "static_framework": true,
  "swift_versions": "5.0",
  "swift_version": "5.0"
}
