{
  "name": "BizConfSDK",
  "version": "1.0.0",
  "summary": "会畅通讯会议SDK",
  "description": "会畅通讯会议SDK是一款云视频会议产品，满足企业全员对远程协同办公的高品质视频会议需要且具备音视频互动，屏幕共享，会议安全，企业直播，电话入会及同声传译等功能；支持全场景终端覆盖，包括Windows、Android、iOS、macOS及会议室终端",
  "homepage": "https://github.com/BizconfVC",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "liuhongfei": "hongfei_liu@bizconf.cn"
  },
  "source": {
    "git": "https://github.com/BizconfVC/BizConfSDK.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64 armv7 x86_64",
    "DEFINES_MODULE": "YES"
  },
  "user_target_xcconfig": {
    "VALID_ARCHS": "arm64 armv7 x86_64",
    "DEFINES_MODULE": "YES"
  },
  "vendored_frameworks": "BizConfSDK/Classes/BizConfSDK.xcframework",
  "dependencies": {
    "HFUtils": [
      "0.1.4"
    ],
    "HFLogBeaver": [
      "1.0.0"
    ],
    "HFNetWorking": [
      "0.1.1"
    ],
    "FcrUIScene": [
      "3.4.2.1"
    ]
  }
}
