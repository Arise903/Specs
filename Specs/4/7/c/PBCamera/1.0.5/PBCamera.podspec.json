  "dependencies": {
    "PBUtil": [

    ],
    "PBLogManage": [

    ],
    "RxSwift": [
      "6.2.0"
    ],
    "RxCocoa": [
      "6.2.0"
    ],
    "YYModel": [
      "1.0.4"
    ],
    "ReactiveObjC": [
      "3.1.0"
    ],
    "Reachability": [
      "3.2"
    ]
  },
  "subspecs": [
    {
      "name": "no-arc",
