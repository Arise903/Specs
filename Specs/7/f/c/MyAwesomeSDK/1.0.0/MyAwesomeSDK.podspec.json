{
  "name": "MyAwesomeSDK",
  "version": "1.0.0",
  "summary": "A simple logging SDK for iOS applications.",
  "description": "MyAwesomeSDK provides simple logging functionality for iOS applications.\nIt allows developers to easily log messages with a custom prefix.",
  "homepage": "https://github.com/sakib-shadman/MyAwesomeSDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Shadman Sakib": "sakibkuetcse@gmail.com"
  },
  "source": {
    "git": "https://github.com/sakib-shadman/MyAwesomeSDK.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "MyAwesomeSDK/**/*.{h,m}",
  "public_header_files": "MyAwesomeSDK/**/*.h",
  "frameworks": "Foundation",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
