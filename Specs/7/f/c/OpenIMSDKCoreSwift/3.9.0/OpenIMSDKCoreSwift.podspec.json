{
  "name": "OpenIMSDKCoreSwift",
  "version": "3.9.0",
  "summary": "OpenIM: An instant messaging (IM) SDK developed by former WeChat engineers, based on Go. The iOS version of the IM SDK can easily replace third-party IM cloud services and build apps with chat and social functions.",
  "homepage": "https://github.com/openimsdk/open-im-sdk-ios",
  "license": {
    "type": "MIT",
    "file": "Sources/LICENSE"
  },
  "authors": {
    "openim": "https://github.com/openimsdk/openim-sdk-swift-core"
  },
  "source": {
    "git": "https://github.com/std-s/openim-sdk-swift-core.git",
    "tag": "3.9.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": [
    "5.7"
  ],
  "source_files": "Sources/**/*.{h,m,swift}",
  "frameworks": "Foundation",
  "libraries": "resolv",
  "static_framework": true,
  "vendored_frameworks": "Framework/*.xcframework",
  "resource_bundles": {
    "Privacy": [
      "Sources/OpenIMSDKCoreSwift/PrivacyInfo.xcprivacy"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "arm64"
  },
  "xcconfig": {
    "ONLY_ACTIVE_ARCH": "YES"
  },
  "swift_version": "5.7"
}
