  "homepage": "https://github.com/RanfengPub/adrfmediation-sdk-ios-pod-masterPub.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "business@ssdmobile.cn": "business@ssdmobile.cn"
  },
  "source": {
    "git": "https://github.com/RanfengPub/adrfmediation-sdk-ios-pod-masterPub.git",
    "tag": "3.9.9.12032"
  },
