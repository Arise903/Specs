{
  "name": "Teak",
  "version": "4.3.3-beta6",
  "summary": "Teak provides rewarded push notifications, emails, and links for free to play games.",
  "homepage": "https://teak.io",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "Alex Scarborough": "alex@teak.io"
  },
  "source": {
    "git": "https://github.com/GoCarrot/teak-ios.git",
    "tag": "4.3.3-beta5"
  },
  "platforms": {
    "ios": "11.0"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": [
        "Teak",
        "Teak/**/*.{h,m,c}"
      ],
      "frameworks": [
        "AdSupport",
        "AVFoundation",
        "ImageIO",
        "CoreServices",
        "StoreKit",
        "UserNotifications",
        "CoreGraphics",
        "UIKit",
        "SystemConfiguration"
      ]
    },
    {
      "name": "Extensions",
      "source_files": [
        "Teak/Extensions/*.{h,m}",
        "Teak/TeakHelpers.h",
        "Teak/TeakHelpers.m",
        "Teak/TeakHealthCheck.m",
        "Teak/3rdParty/UIImage+animatedGIF.m"
      ],
      "frameworks": "UserNotifications"
    }
  ]
}
