  "homepage": "https://github.com/erduoniba/A00CppInitMeasure",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "denglibing": "denglibing@gmail.com"
  },
  "source": {
    "git": "https://github.com/erduoniba/A00CppInitMeasure.git",
    "tag": "1.0.1-beta1"
  },
  "platforms": {
    "ios": "9.0"
  },
  "static_framework": true,
  "ios": {
    "libraries": "c++"
  },
  "vendored_frameworks": "A00CppInitMeasure/Frameworks/A00CppInitMeasure.xcframework",
  "preserve_paths": [
    "A00CppInitMeasure/Frameworks/A00CppInitMeasure.xcframework",
    "A00CppInitMeasure/Classes/**/*"
  ]
