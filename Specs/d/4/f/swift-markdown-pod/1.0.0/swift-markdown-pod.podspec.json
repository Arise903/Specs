{
  "name": "swift-markdown-pod",
  "version": "1.0.0",
  "summary": "A Swift package for parsing, building, editing, and analyzing Markdown documents.",
  "description": "Swift Markdown is a Swift package for parsing, building, editing, and analyzing Markdown documents.\nThe parser is powered by GitHub-flavored Markdown's cmark-gfm implementation, so it follows the spec closely.\nThe markup tree provided by this package is comprised of immutable/persistent, thread-safe, copy-on-write value types.",
  "homepage": "https://github.com/GIKICoder/swift-markdown",
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE.txt"
  },
  "authors": {
    "GIKICoder": "giki.biu@gmail.com"
  },
  "source": {
    "git": "https://github.com/GIKICoder/swift-markdown.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.7",
  "source_files": "Sources/Markdown/**/*.swift",
  "dependencies": {
    "swift-cmark-pod": [

    ]
  },
  "frameworks": "Foundation",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/CAtomic/include"
  },
  "subspecs": [
    {
      "name": "CAtomic",
      "source_files": "Sources/CAtomic/**/*.{c,h}",
      "public_header_files": "Sources/CAtomic/include/*.h",
      "preserve_paths": "Sources/CAtomic/include/module.modulemap",
      "pod_target_xcconfig": {
        "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/Sources/CAtomic/include"
      }
    }
  ],
  "swift_version": "5.7"
}
