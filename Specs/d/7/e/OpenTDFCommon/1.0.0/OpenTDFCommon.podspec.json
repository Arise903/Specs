{
  "name": "OpenTDFCommon",
  "version": "1.0.0",
  "summary": "Supports Module ecosystem reuse for Kuikly and Hippy.",
  "description": "- tdf 公共代码库\n- Modules extended by TDFModule are available for Kuikly and Hippy to share\n- and existing Hippy Modules can also be quickly migrated to Kuikly",
  "homepage": "https://kuikly.tds.qq.com/",
  "license": {
    "type": "Apache",
    "file": "OpenTDFCommon-1.0.0/LICENSE"
  },
  "authors": {
    "jonasluo": "jonasluo@tencent.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "http": "https://github.com/elixxli/OpenTDFCommon/archive/refs/tags/1.0.0.zip"
  },
  "vendored_frameworks": "OpenTDFCommon-1.0.0/TDFCommon.xcframework",
  "pod_target_xcconfig": {
    "VALID_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64"
  },
  "libraries": "c++",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "OpenTDFCommon-1.0.0/TDFCommon.xcframework/ios-arm64/TDFCommon.framework/Headers"
  },
  "user_target_xcconfig": {
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "requires_arc": true,
  "source_files": "OpenTDFCommon-1.0.0/TDFCommon.xcframework/**/*.h"
}
