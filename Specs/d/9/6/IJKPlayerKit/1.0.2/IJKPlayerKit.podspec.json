{
  "name": "IJKPlayerKit",
  "version": "1.0.2",
  "summary": "基于IJKPlayer编译封装的直播播放器(也可作为视屏播放器)，支持RTMP/RTMPS/RTMPT/RTMPE/RTSP/HLS/HTTP(S)-FLV/KMP 等直播协议与MP4、FLV等格式， 支持录屏功能",
  "description": "集成注意事项：\n使用cocoapods官方源\nsource 'https://github.com/CocoaPods/Specs.git'\npod 'IJKPlayerKit'\n\n详细使用见：https://fsplayer.debugly.cn/manuals/getting-started.html",
  "authors": {
    "官人": "aiguanren@icloud.com"
  },
  "homepage": "https://github.com/aiguanren/WYBasisKit-swift",
  "documentation_url": "https://fsplayer.debugly.cn/manuals/getting-started.html",
  "license": {
    "type": "MIT",
    "file": "FSPlayer/License.md"
  },
  "resource_bundles": {
    "IJKPlayerKit": [
      "FSPlayer/PrivacyInfo.xcprivacy"
    ]
  },
  "platforms": {
    "ios": "11.0",
    "tvos": "12.0",
    "osx": "10.11"
  },
  "source": {
    "http": "https://github.com/aiguanren/IJK/releases/download/1.0.2/FSPlayer.zip"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "vendored_frameworks": "FSPlayer/FSPlayer/FSPlayer.xcframework",
  "libraries": [
    "c++",
    "z",
    "bz2",
    "iconv",
    "xml2",
    "lzma"
  ],
  "frameworks": [
    "AVFoundation",
    "AudioToolbox",
    "CoreMedia",
    "CoreVideo",
    "VideoToolbox",
    "Metal"
  ],
  "osx": {
    "frameworks": [
      "Cocoa",
      "AudioUnit",
      "OpenGL",
      "GLKit",
      "CoreImage"
    ]
  },
  "tvos": {
    "frameworks": [
      "UIKit",
      "OpenGLES"
    ]
  },
  "ios": {
    "frameworks": [
      "UIKit",
      "OpenGLES"
    ]
  },
  "swift_version": "5.0"
}
