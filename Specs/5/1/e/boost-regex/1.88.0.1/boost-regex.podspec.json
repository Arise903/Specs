{
  "name": "boost-regex",
  "version": "1.88.0.1",
  "summary": "Boost C++ libraries for macOS, iOS, tvOS including builds for Mac Catalyst, iOS Simulator, tvOS Simulator.",
  "homepage": "https://github.com/resoul/boost-regex",
  "license": "Boost Software License",
  "authors": {
    "Alexander Pototskiy": "alex.a.potocki@gmail.com"
  },
  "platforms": {
    "ios": "13.4",
    "osx": "11.0",
    "tvos": "13.0"
  },
  "ios": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    },
    "user_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "osx": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    },
    "user_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "tvos": {
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    },
    "user_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "YES"
    }
  },
  "static_framework": true,
  "source": {
    "git": "https://github.com/resoul/boost-regex.git",
    "tag": "1.88.0.1"
  },
  "header_mappings_dir": "Source/Headers",
  "default_subspecs": "regex",
  "subspecs": [
    {
      "name": "regex",
      "source_files": "Source/Headers/**/*.{h,hpp,ipp}",
      "vendored_frameworks": "Source/boost_regex.xcframework"
    }
  ]
}
