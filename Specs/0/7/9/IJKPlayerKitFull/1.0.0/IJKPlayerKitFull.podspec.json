{
  "name": "IJKPlayerKitFull",
  "version": "1.0.0",
  "summary": "基于IJKPlayer编译封装的直播播放器(也可作为视屏播放器)，支持RTMP/RTMPS/RTMPT/RTMPE/RTSP/HLS/HTTP(S)-FLV/KMP 等直播协议与MP4、FLV等格式， 支持录屏功能",
  "description": "集成注意事项：\n使用cocoapods官方源\nsource 'https://github.com/CocoaPods/Specs.git'\npod 'IJKPlayerKitFull'",
  "authors": {
    "官人": "aiguanren@icloud.com"
  },
  "homepage": "https://github.com/aiguanren/WYBasisKit-swift",
  "license": {
    "type": "MIT",
    "file": "IJKPlayerKit/License.md"
  },
  "resource_bundles": {
    "IJKPlayerKit": [
      "IJKPlayerKit/PrivacyInfo.xcprivacy"
    ]
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "http": "https://github.com/aiguanren/IJKPlayerKit/releases/download/1.0.0/IJKPlayerKit.zip"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "static_framework": true,
  "vendored_frameworks": "IJKPlayerKit/arm64&x86_64/IJKMediaPlayer.xcframework",
  "libraries": [
    "c++",
    "z",
    "bz2"
  ],
  "frameworks": [
    "UIKit",
    "AudioToolbox",
    "CoreGraphics",
    "AVFoundation",
    "CoreMedia",
    "CoreVideo",
    "MediaPlayer",
    "CoreServices",
    "Metal",
    "QuartzCore",
    "VideoToolbox"
  ],
  "swift_version": "5.0"
}
