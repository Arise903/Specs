{
  "name": "WangIMMapKit",
  "version": "0.0.1",
  "summary": "在IM中使用高德地图，进行位置共享和分享位置,使用NO-IDFA避免上架问题",
  "description": "提供在IM应用中集成高德地图的功能，支持实时位置共享与位置消息发送，使用无IDFA版本SDK规避App Store审核问题。",
  "homepage": "https://github.com/Wang/WangIMMapKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Wang": "1162558647@qq.com"
  },
  "source": {
    "git": "git@github.com:WXLiang-17/WangIMMapKit.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source_files": "WangIMMapKit/Classes/**/*",
  "resource_bundles": {
    "WangIMMapKit": [
      "WangIMMapKit/Assets/*.xcassets",
      "WangIMMapKit/Assets/**/*.png",
      "WangIMMapKit/Assets/**/*.json"
    ]
  },
  "dependencies": {
    "Kingfisher": [

    ],
    "SnapKit": [

    ],
    "AMap3DMap-NO-IDFA": [

    ],
    "AMapSearch-NO-IDFA": [

    ],
    "AMapLocation-NO-IDFA": [

    ]
  },
  "static_framework": true,
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -framework \"MAMapKit\" -framework \"AMapSearchKit\" -framework \"AMapLocationKit\"",
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/AMap3DMap-NO-IDFA\" \"$(PODS_ROOT)/AMapSearch-NO-IDFA\" \"$(PODS_ROOT)/AMapLocation-NO-IDFA\""
  },
  "swift_version": "5.0"
}
