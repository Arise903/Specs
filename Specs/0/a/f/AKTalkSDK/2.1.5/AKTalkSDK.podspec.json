{
  "name": "AKTalkSDK",
  "version": "2.1.5",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "A lightweight scanning component based on system API in Swift5",
  "description": "No memory leaks, Support scaling, Support light sense to automatically turn on the flash, Support auto zoom, Support custom UI.",
  "authors": {
    "zhongbin": "zhongbin.wen@akuvox.com"
  },
  "homepage": "https://github.com/Akuvox/AKTalkSDK",
  "source": {
    "git": "https://github.com/Akuvox/AKTalkSDK.git",
    "tag": "2.1.5"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "12.0"
  },
  "static_framework": true,
  "ios": {
    "vendored_frameworks": "AKTalkSDK/AKTalkSDK.framework"
  },
  "source_files": "AKTalkSDK/**/*.h",
  "public_header_files": "AKTalkSDK/AKTalkSDK.framework/Headers/*.h",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "SWIFT_OPTIMIZATION_LEVEL": "-Onone",
    "SWIFT_VERSION": "5.0",
    "VALID_ARCHS": "x86_64 armv7 arm64"
  }
}
