{
  "name": "nimbbl_mobile_kit_ios_core_api_sdk",
  "version": "1.0.7",
  "summary": "Nimbbl Core API SDK for iOS",
  "description": "Core API SDK for Nimbbl payment integration on iOS.",
  "homepage": "https://github.com/nimbbl-tech/nimbbl_mobile_kit_ios_core_api_sdk",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Nimbbl": "tech@nimbbl.biz"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/nimbbl-tech/nimbbl_mobile_kit_ios_core_api_pod.git",
    "tag": "1.0.7"
  },
  "static_framework": true,
  "source_files": "static_lib/Headers/*.h",
  "public_header_files": "static_lib/Headers/*.h",
  "requires_arc": false,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "MACH_O_TYPE": "staticlib",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "NO",
    "ENABLE_BITCODE": "NO",
    "CODE_SIGNING_ALLOWED": "NO",
    "CODE_SIGNING_REQUIRED": "NO",
    "ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "NO",
    "EMBEDDED_CONTENT_CONTAINS_SWIFT": "NO",
    "LIBRARY_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/nimbbl_mobile_kit_ios_core_api_sdk/static_lib",
    "OTHER_LDFLAGS": "$(inherited) -lnimbbl_mobile_kit_ios_core_api_sdk"
  },
  "user_target_xcconfig": {
    "MACH_O_TYPE": "staticlib",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "NO",
    "ENABLE_BITCODE": "NO",
    "ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES": "NO",
    "EMBEDDED_CONTENT_CONTAINS_SWIFT": "NO",
    "LIBRARY_SEARCH_PATHS": "$(inherited) $(PODS_ROOT)/nimbbl_mobile_kit_ios_core_api_sdk/static_lib",
    "OTHER_LDFLAGS": "$(inherited) -lnimbbl_mobile_kit_ios_core_api_sdk"
  }
}
