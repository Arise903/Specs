{
  "name": "ReerCodable",
  "version": "1.2.1",
  "summary": "Codable extensions using Swift Macro",
  "description": "Enhancing Swift's Codable Protocol Using Macros: A Declarative Approach to Serialization",
  "homepage": "https://github.com/reers/ReerCodable",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Asura19": "x.rhythm@qq.com"
  },
  "source": {
    "git": "https://github.com/reers/ReerCodable.git",
    "tag": "1.2.1"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15",
    "watchos": "6.0",
    "tvos": "13.0",
    "visionos": "1.0"
  },
  "swift_versions": "5.10",
  "source_files": "Sources/**/*",
  "preserve_paths": [
    "Sources/Resources/ReerCodableMacros"
  ],
  "exclude_files": "Sources/ReerCodableMacros",
  "pod_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/ReerCodable/Sources/Resources/ReerCodableMacros#ReerCodableMacros"
  },
  "user_target_xcconfig": {
    "OTHER_SWIFT_FLAGS": "-enable-experimental-feature SymbolLinkageMarkers -Xfrontend -load-plugin-executable -Xfrontend ${PODS_ROOT}/ReerCodable/Sources/Resources/ReerCodableMacros#ReerCodableMacros"
  },
  "swift_version": "5.10"
}
