{
  "name": "OpenWeb3Lib60",
  "version": "1.0.5",
  "summary": "OpenWebLib is a powerful library for interacting with Web3 technologies.",
  "description": "OpenWebLib is a comprehensive library that enables seamless interaction with Web3 technologies,\n                                    offering features like smart contract integration, blockchain data querying, and more.",
  "homepage": "https://docs.openweb3.io/docs/getting-started",
  "license": "MIT",
  "platforms": {
    "ios": "13.0"
  },
  "authors": {
    "OpenWeb3": "mtsocialdao@gmail.com"
  },
  "source": {
    "git": "https://github.com/web3user1/OpenWeb3-Swift6.0.git",
    "tag": "1.0.5"
  },
  "requires_arc": true,
  "module_map": "Frameworks/module.modulemap",
  "xcconfig": {
    "GENERATE_INFOPLIST_FILE": "YES",
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2 $(PODS_ROOT)/OpenWeb3Lib60/OpenWeb3Lib/Frameworks"
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(PODS_ROOT)/OpenWeb3Lib60/OpenWeb3Lib/Frameworks",
    "VALID_ARCHS": "x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "OpenWeb3Lib",
      "vendored_frameworks": "Frameworks/OpenWeb3Lib.xcframework"
    }
  ]
}
