{
  "name": "BluArmorLibrary",
  "version": "1.0.2",
  "summary": "A library for BluConnect and related frameworks.",
  "description": "BluArmorLibrary integrates multiple frameworks like BluConnect, RTKAudioConnectSDK, \nRTKLEFoundation, and RTKOTASDK, providing seamless connectivity and OTA updates for devices.",
  "homepage": "https://gitlab.com/Piyushkantmaav/bluarmorlibrarycocoapods",
  "license": {
    "type": "MIT"
  },
  "swift_versions": [
    "5.0"
  ],
  "authors": {
    "Piyushkantmaav": "piyush@aptener.com"
  },
  "platforms": {
    "ios": "14.0"
  },
  "dependencies": {
    "NordicDFU": [
      "~> 4.16.0"
    ],
    "SwiftProtobuf": [
      "1.27.0"
    ],
    "Swinject": [
      "2.9.1"
    ]
  },
  "vendored_frameworks": [
    "BluArmorLibrary/BluConnect.xcframework",
    "BluArmorLibrary/RTKAudioConnectSDK.xcframework",
    "BluArmorLibrary/RTKLEFoundation.xcframework",
    "BluArmorLibrary/RTKOTASDK.xcframework"
  ],
  "source": {
    "git": "https://gitlab.com/Piyushkantmaav/bluarmorlibrarycocoapods.git",
    "tag": "1.0.2"
