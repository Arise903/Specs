{
  "name": "JAAKFaceDetector",
  "version": "1.0.0",
  "summary": "AI-powered face detection and recording library for iOS using MediaPipe BlazeFace",
  "description": "JAAKFaceDetector is an advanced face detection library that provides:\n- Real-time face detection using MediaPipe BlazeFace\n- Auto-recording with quality analysis\n- Face quality metrics and validation\n- Security monitoring and device validation\n- Customizable UI overlays and instructions\n- Video recording",
  "homepage": "https://github.com/BrunoSolis5/jaak-face-detector-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Diego Bruno": "diego.bruno@jaak.ai"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "http": "https://github.com/BrunoSolis5/jaak-face-detector-ios/releases/download/v1.0.0/JAAKFaceDetector-1.0.0.zip"
  },
  "source_files": "JAAKFaceDetector/**/*.swift",
  "exclude_files": "JAAKFaceDetector/Info.plist",
  "resource_bundles": {
    "JAAKFaceDetector": [
      "JAAKFaceDetector/Resources/**/*"
    ]
  },
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Vision",
    "CoreML"
  ],
  "dependencies": {
    "MediaPipeTasksVision": [
      "~> 0.10.3"
    ]
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
    "IPHONEOS_DEPLOYMENT_TARGET": "12.0",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "deprecated_in_favor_of": "JAAKVisage",
  "swift_version": "5.0"
}
