{
  "name": "opencv2-RNE",
  "version": "0.1.0",
  "summary": "RNE-compatible OpenCV (Computer Vision) for iOS.",
  "homepage": "https://github.com/jakmro/opencv2.git",
  "authors": {
    "Jakub Mroz": "jakub.mroz@swmansion.com"
  },
  "license": {
    "type": "Apache License 2.0",
    "file": "LICENSE"
  },
  "source": {
    "git": "https://github.com/jakmro/opencv2.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "16.0"
  },
  "preserve_paths": "opencv2.xcframework",
  "vendored_frameworks": "opencv2.xcframework",
  "requires_arc": false,
  "ios": {
    "frameworks": [
      "AssetsLibrary",
      "AVFoundation",
      "CoreGraphics",
      "CoreMedia",
      "CoreVideo",
      "Foundation",
      "QuartzCore",
      "UIKit"
    ]
  },
  "libraries": [
    "c++",
    "stdc++"
  ],
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "CLANG_CXX_LIBRARY": "libc++"
  }
}
