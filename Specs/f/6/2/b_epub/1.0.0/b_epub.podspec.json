{
  "name": "b_epub",
  "version": "1.0.0",
  "summary": "A simple EPUB package for reading EPUB files.",
  "description": "This library provides an easy way to read EPUB files, leveraging powerful tools like FolioReaderKit.\nIncludes features such as customizable UI, font rendering, and advanced EPUB parsing.",
  "homepage": "https://github.com/anjalmaharjan/b_epub",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "anjalmaharjan": "heromaharjan@gmail.com"
  },
  "source": {
    "git": "https://github.com/anjalmaharjan/b_epub.git",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source_files": [
    "FolioReaderKit/Source/*.{h,swift}",
    "FolioReaderKit/Source/**/*.swift",
    "FolioReaderKit/Vendor/**/*.swift"
  ],
  "resources": [
    "FolioReaderKit/Source/**/*.{js,css}",
    "FolioReaderKit/Source/Resources/*.xcassets",
    "FolioReaderKit/Source/Resources/Fonts/**/*.{otf,ttf}"
  ],
  "public_header_files": "FolioReaderKit/Source/*.h",
  "libraries": "z",
  "dependencies": {
    "SSZipArchive": [
      "2.2.3"
    ],
    "MenuItemKit": [
      "3.1.3"
    ],
    "ZFDragableModalTransition": [
      "0.6"
    ],
    "AEXML": [
      "4.3.3"
    ],
    "FontBlaster": [
      "4.1.0"
    ],
    "RealmSwift": [
      "~> 10.40.0"
    ],
    "Realm": [
      "~> 10.40.0"
    ]
  },
  "swift_version": "5.0"
}
