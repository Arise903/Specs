{
  "name": "SFmeetRTCiOS",
  "version": "1.0.0",
  "summary": "SFmeetRTCiOS Toolkit.",
  "description": "The iOS Toolkit needs be used to build native iOS Application to integrate Real Time Communication using SmartfloMeet  Platform. This toolkit exposes functions, methods, notifications required to communicate with SmartfloMeet Signalling Servers and Media Servers using a Valid Token to carry out a RTC Session.",
  "homepage": "https://doc.smartflomeet.ttns.in/developer/video-api/client-api/ios-toolkit/",
  "authors": {
    "smartflomeet": "https://github.com/smartflomeet"
  },
  "license": {
    "type": "Tata Tele License, Version 1.0",
    "text": "https://www.tatatelebusiness.com/terms-and-conditions/"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "http": "https://doc.smartflomeet.ttns.in/developer/wp-content/uploads/video-sdk/ios/EnxRTCiOS.xcframework_3.0.19.zip"
  },
  "ios": {
    "vendored_frameworks": "EnxRTCiOS.xcframework",
    "dependencies": {
      "SFmeetWebRTC": [
        "1.0.0"
      ]
    }
  },
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.4",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64",
    "ENABLE_BITCODE": "NO"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
