{
  "name": "KeyWordDetection",
  "version": "1.0.58",
  "summary": "Wake-word detection for iOS / React-Native",
  "description": "Static XCFramework + optional React-Native bridge.",
  "homepage": "https://github.com/frymanofer/WakeWords-Voice-Commands-iOS-Cocoa-Pods-Sources",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Ofer Fryman": "ofer@davoice.io"
  },
  "cocoapods_version": ">= 1.10.0",
  "platforms": {
    "ios": "13.5"
  },
  "swift_versions": "5.0",
  "static_framework": true,
  "source": {
    "git": "https://github.com/frymanofer/WakeWords-Voice-Commands-iOS-Cocoa-Pods-Sources.git",
    "tag": "1.0.58"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "vendored_frameworks": "KeyWordDetection.xcframework",
      "resource_bundles": {
        "KeyWordDetectionPrivacy": [
          "ios/Resources/PrivacyInfo.xcprivacy"
        ]
      },
      "resources": "ios/models/*",
      "dependencies": {
        "onnxruntime-objc": [
          "~> 1.20.0"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
