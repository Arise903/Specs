{
  "name": "JAAKVisage",
  "version": "1.0.0-beta.1",
  "summary": "AI-powered face detection and recording library for iOS using MediaPipe BlazeFace",
  "description": "JAAKVisage is an advanced face detection library that provides:\n- Real-time face detection using MediaPipe BlazeFace\n- Auto-recording with quality analysis\n- Face quality metrics and validation\n- Security monitoring and device validation\n- Customizable UI overlays and instructions\n- Video recording",
  "homepage": "https://github.com/jaak-ai/jaak-visage-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Diego Bruno": "diego.bruno@jaak.ai"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "git": "https://github.com/jaak-ai/jaak-visage-ios.git",
    "tag": "v1.0.0-beta.1"
  },
  "source_files": "JAAKVisage/**/*.swift",
  "exclude_files": "JAAKVisage/Info.plist",
  "resource_bundles": {
    "JAAKVisage": [
      "JAAKVisage/Resources/**/*"
    ]
  },
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Vision",
    "CoreML"
  ],
  "dependencies": {
    "MediaPipeTasksVision": [
      "~> 0.10.3"
    ]
  },
  "requires_arc": true,
  "static_framework": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
    "IPHONEOS_DEPLOYMENT_TARGET": "12.0",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386"
  },
  "swift_version": "5.0"
}
