{
  "name": "DDKitSwift",
  "swift_versions": "5.0",
  "version": "4.0.3",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "DDKitSwift is a debugging SDK tool designed for iOS devices. It integrates a variety of practical debugging features to help developers and users identify and troubleshoot device issues.",
  "homepage": "https://github.com/DamonHu/DDKitSwift",
  "authors": {
    "DDKitSwift": "dong765@qq.com"
  },
  "source": {
    "git": "https://github.com/DamonHu/DDKitSwift.git",
    "tag": "4.0.3"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "12.0"
  },
  "default_subspecs": "core",
  "documentation_url": "https://dongge.org/blog/1307.html",
  "subspecs": [
    {
      "name": "core",
      "resource_bundles": {
        "DDKitSwift": [
          "pod/assets/**/*"
        ]
      },
      "source_files": [
        "pod/*.swift",
        "pod/**/*.swift"
      ],
      "dependencies": {
        "DDUtils/ui": [
          "~> 5"
        ],
        "DDUtils/utils": [
          "~> 5"
        ],
        "DDLoggerSwift": [
          "~> 5"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
