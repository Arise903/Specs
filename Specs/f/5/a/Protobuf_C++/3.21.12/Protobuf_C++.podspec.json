{
  "name": "Protobuf_C++",
  "version": "3.21.12",
  "summary": "Protocol Buffers v3 runtime library for C++.",
  "homepage": "https://github.com/jkeyi/protobuf.git",
  "license": "BSD-3-Clause",
  "authors": {
    "The Protocol Buffers contributors": "jkeyi@qq.com"
  },
  "cocoapods_version": ">= 1.0",
  "source": {
    "git": "https://github.com/jkeyi/protobuf.git",
    "tag": "v3.21.12"
  },
  "source_files": [
    "src/google/protobuf/*.{h,cc,inc}",
    "src/google/protobuf/stubs/*.{h,cc}",
    "src/google/protobuf/io/*.{h,cc}",
    "src/google/protobuf/util/*.{h,cc}",
    "src/google/protobuf/util/internal/*.{h,cc}"
  ],
  "exclude_files": [
    "src/google/**/*_test.{h,cc,inc}",
    "src/google/**/*_unittest.{h,cc}",
    "src/google/protobuf/test_util*.{h,cc}",
    "src/google/protobuf/map_lite_test_util.{h,cc}",
    "src/google/protobuf/map_test_util*.{h,cc,inc}",
    "src/google/protobuf/reflection_tester.{h,cc}"
  ],
  "header_mappings_dir": "src",
  "platforms": {
    "ios": "13.0"
  },
  "pod_target_xcconfig": {
    "USE_HEADERMAP": "NO",
    "ALWAYS_SEARCH_USER_PATHS": "NO",
    "HEADER_SEARCH_PATHS": "\"$(PODS_TARGET_SRCROOT)/src\""
  }
}
