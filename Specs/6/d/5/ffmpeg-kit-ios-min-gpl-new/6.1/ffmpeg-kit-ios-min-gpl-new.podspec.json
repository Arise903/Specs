{
  "name": "ffmpeg-kit-ios-min-gpl-new",
  "version": "6.1",
  "summary": "FFmpeg Kit iOS Min GPL Shared Framework",
  "description": "Includes FFmpeg with libvid.stab, x264, x265 and xvidcore libraries enabled.\",\n  \"homepage\": \"https://github.com/yosizesta/ffmpeg-kit",
  "authors": {
    "Yosi Oren": "yosi@zesta.ai"
  },
  "homepage": "https://zesta.ai",
  "source": {
    "git": "https://bitbucket.org/zaffic/ffmpeg-kit-ios-min-gpl-new.git",
    "tag": "6.1"
  },
  "license": {
    "type": "GPL-3.0-or-later",
    "file": "ffmpegkit.xcframework/ios-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "ios": "12.1"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "resources": "*.xcframework",
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework"
    ]
  }
}
