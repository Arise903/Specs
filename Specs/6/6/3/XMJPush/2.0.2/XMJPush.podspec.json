{
  "name": "XMJPush",
  "version": "2.0.2",
  "summary": "A short description of XMJPush.",
  "description": "2.0.1 has been used now, welcome join us.",
  "homepage": "https://github.com/huajis/XMJPush",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "": ""
  },
  "platforms": {
    "ios": "12.0"
  },
  "source": {
    "git": "https://github.com/huajis/XMJPush.git",
    "tag": "2.0.2"
  },
  "source_files": [
    "Classes",
    "Classes/**/*.{h,m}"
  ],
  "exclude_files": "Classes/Exclude",
  "frameworks": [
    "Foundation",
    "UIKit"
  ],
  "static_framework": true,
  "requires_arc": true,
  "dependencies": {
    "JPush": [
      "~> 3.3.4"
    ],
    "JShare": [
      "~> 1.9.0"
    ]
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "VALID_ARCHS": "x86_64 arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "GCC_WARN_INHIBIT_ALL_WARNINGS": "YES"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
