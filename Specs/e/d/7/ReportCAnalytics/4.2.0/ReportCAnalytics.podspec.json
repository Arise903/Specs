{
  "name": "ReportCAnalytics",
  "version": "4.2.0",
  "summary": "iOS SDK of GrowingIO.",
  "description": "ReportCAnalytics具备自动采集基本的用户行为事件，比如访问和行为数据等。目前支持代码埋点、无埋点、可视化圈选、热图等功能。",
  "homepage": "https://www.growingio.com/",
  "license": {
    "type": "Apache2.0",
    "file": "LICENSE"
  },
  "authors": {
    "GrowingIO": "support@growingio.com"
  },
  "source": {
    "git": "https://gitee.com/tim0523/report-sdk-ios-ytd-tracker_V1.git",
    "tag": "4.2.0"
  },
  "platforms": {
    "ios": "10.0"
  },
  "ios": {
    "frameworks": "WebKit"
  },
  "requires_arc": true,
  "default_subspecs": "Autotracker",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
  },
  "subspecs": [
    {
      "name": "Autotracker",
      "platforms": {
        "ios": "10.0"
      },
      "source_files": "GrowingAutotracker/**/*{.h,.m,.c,.cpp,.mm}",
      "public_header_files": "GrowingAutotracker/*.h",
      "dependencies": {
        "ReportCAnalytics/AutotrackerCore": [
          "4.2.0"
        ],
        "ReportCAnalytics/DefaultServices": [
          "4.2.0"
        ]
      },
      "ios": {
        "dependencies": {
          "ReportCAnalytics/Hybrid": [
            "4.2.0"
          ],
          "ReportCAnalytics/MobileDebugger": [
            "4.2.0"
          ],
          "ReportCAnalytics/WebCircle": [
            "4.2.0"
          ]
        }
      }
    },
    {
      "name": "Tracker",
      "source_files": "GrowingTracker/**/*{.h,.m,.c,.cpp,.mm}",
      "public_header_files": "GrowingTracker/*.h",
      "dependencies": {
        "ReportCAnalytics/TrackerCore": [
          "4.2.0"
        ],
        "ReportCAnalytics/DefaultServices": [
          "4.2.0"
        ]
      },
      "ios": {
        "dependencies": {
          "ReportCAnalytics/MobileDebugger": [
            "4.2.0"
          ]
        }
      }
    },
    {
      "name": "TrackerCore",
      "dependencies": {
        "GrowingUtils/TrackerCore": [
          "~> 1.1.0"
        ]
      },
      "source_files": "GrowingTrackerCore/**/*{.h,.m,.c,.cpp,.mm}",
      "public_header_files": "GrowingTrackerCore/Public/*.h",
      "ios": {
        "resource_bundles": {
          "ReportCAnalytics": [
            "Resources/iOS/GrowingAnalytics.bundle/PrivacyInfo.xcprivacy"
          ]
        }
      },
      "osx": {
        "resource_bundles": {
          "ReportCAnalytics": [
            "Resources/macOS/GrowingAnalytics.bundle/PrivacyInfo.xcprivacy"
          ]
        }
      },
      "libraries": "c++"
    },
    {
      "name": "AutotrackerCore",
      "platforms": {
        "ios": "10.0"
      },
      "dependencies": {
        "GrowingUtils/AutotrackerCore": [
          "~> 1.1.0"
        ],
        "ReportCAnalytics/TrackerCore": [
          "4.2.0"
