{
  "name": "FUICore",
  "version": "0.0.2",
  "summary": "配合OpenIMSDK的iOS原生界面",
  "description": "OpenIM：由前微信技术专家打造的基于 Go 实现的即时通讯（IM）项目，iOS版本IM SDK 可以轻松替代第三方IM云服务，打造具备聊天、社交功能的app。",
  "homepage": "https://github.com/olstory/FUICore",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "olstory": "olimstory@163.com"
  },
  "source": {
    "git": "https://github.com/olstory/FUICore.git",
    "tag": "0.0.2"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.0",
  "source_files": "FUICore/Classes/**/*",
  "resource_bundles": {
    "FIMUIResource": [
      "FUICore/Assets/OIMUIResource.bundle/*.*"
    ],
    "FIMUIEmoji": [
      "FUICore/Assets/OIMUIEmoji.bundle/*"
    ]
  },
  "static_framework": true,
  "dependencies": {
    "OpenIMSDK": [
      "3.5.1"
    ],
    "SnapKit": [
      "~> 5.6.0"
    ],
    "RxSwift": [
      "~> 6.5"
    ],
    "RxCocoa": [
      "~> 6.0"
    ],
    "RxKeyboard": [
      "~> 2.0"
    ],
    "RxDataSources": [
      "5.0.0"
    ],
    "Kingfisher": [
      "7.7.0"
    ],
    "lottie-ios": [
      "~> 3.0"
    ],
    "ZLPhotoBrowser": [
      "4.3.0"
    ],
    "ProgressHUD": [
      "14.1.0"
    ],
    "IQKeyboardManagerSwift": [

    ],
    "Localize-Swift": [
      "~> 3.2"
    ],
    "Alamofire": [
      "5.6.4"
    ]
  },
  "public_header_files": "FUICore/Classes/**/*.h",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64 x86_64 armv7"
  },
  "swift_version": "5.0"
}
