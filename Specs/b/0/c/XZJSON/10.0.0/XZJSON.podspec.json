  "homepage": "https://github.com/Xezun/XZKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Xezun": "xezun@icloud.com"
  },
  "source": {
    "git": "https://github.com/Xezun/XZKit.git",
    "tag": "10.0.0"
  },
  "platforms": {
    "ios": "12.0"
  },
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "XZ_FRAMEWORK=1"
  },
  "deprecated_in_favor_of": "XZKit/XZJSON",
  "subspecs": [
    {
      "name": "Code",
      "source_files": "XZKit/Code/XZJSON/**/*.{h,m}",
