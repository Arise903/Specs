{
  "name": "MPVKit",
  "version": "0.40.0",
  "summary": "MPVKit",
  "description": "MPV Player for iOS and macOS",
  "homepage": "https://github.com/Alexk2309/MPVKit.git",
  "authors": {
    "alexkim": "alexkim5682@gmail.com"
  },
  "license": "MIT",
  "source": {
    "git": "https://github.com/Alexk2309/MPVKit.git",
    "tag": "0.40.0"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15",
    "tvos": "13.0"
  },
  "default_subspecs": "MPVKit",
  "static_framework": true,
  "source_files": "Sources/_MPVKit/**/*.{h,c,m}",
  "preserve_paths": "Frameworks/**/*",
  "subspecs": [
    {
      "name": "MPVKit",
      "libraries": [
        "bz2",
        "c++",
        "iconv",
        "resolv",
        "xml2",
        "z"
      ],
      "osx": {
        "libraries": "expat",
