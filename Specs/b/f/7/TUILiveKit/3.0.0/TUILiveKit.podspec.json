{
  "name": "TUILiveKit",
  "version": "3.0.0",
  "platforms": {
    "ios": "13.0"
  },
  "license": {
    "type": "MIT",
    "file": "iOS/TUILiveKit/LICENSE"
  },
  "homepage": "https://trtc.io/",
  "documentation_url": "https://trtc.io/document",
  "authors": "trtc.io",
  "summary": "trtc.io for Live streaming Solution..",
  "static_framework": true,
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i386 x86_64 arm64"
  },
  "source": {
    "git": "https://github.com/Tencent-RTC/TUILiveKit.git",
    "tag": "v3.0.0"
  },
  "dependencies": {
    "SnapKit": [

    ],
    "TUICore": [

    ],
    "Kingfisher": [

    ],
    "SVGAPlayer": [
      "~> 2.5.7"
    ],
    "Protobuf": [
      "~> 3.22.1"
    ],
    "RTCCommon": [
      ">= 1.2.1"
    ],
    "ESPullToRefresh": [

    ]
  },
  "default_subspecs": "Professional",
  "subspecs": [
    {
      "name": "Professional",
      "dependencies": {
        "RTCRoomEngine/Professional": [
          ">= 3.0.0"
        ],
        "LiveStreamCore/Professional": [
          ">= 3.0.3"
        ]
      },
      "source_files": [
        "iOS/TUILiveKit/Sources/**/*",
        "iOS/TUILiveKit/VideoLiveKit.swift",
        "iOS/TUILiveKit/VoiceRoomKit.swift",
        "iOS/TUILiveKit/VoiceRoomDefine.swift"
      ],
      "resource_bundles": {
        "TUILiveKitBundle": [
          "iOS/TUILiveKit/Resources/*.xcassets",
          "iOS/TUILiveKit/Resources/Localized/**/*.strings"
        ]
      }
    },
    {
      "name": "TRTC",
      "dependencies": {
        "RTCRoomEngine/TRTC": [
          ">= 3.0.0"
        ],
        "LiveStreamCore/TRTC": [
          ">= 3.0.3"
        ]
      },
      "source_files": [
        "iOS/TUILiveKit/Sources/**/*",
        "iOS/TUILiveKit/VideoLiveKit.swift",
        "iOS/TUILiveKit/VoiceRoomKit.swift",
        "iOS/TUILiveKit/VoiceRoomDefine.swift"
      ],
      "resource_bundles": {
        "TUILiveKitBundle": [
          "iOS/TUILiveKit/Resources/*.xcassets",
          "iOS/TUILiveKit/Resources/Localized/**/*.strings"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
