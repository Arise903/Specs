{
  "name": "XXTools_Swift",
  "version": "0.1.3",
  "summary": "iOS开发常用工具类扩展库，提供UIKit组件扩展和实用工具方法",
  "description": "XXTools_Swift是一个专为iOS开发设计的Swift工具库，包含了常用UI组件的扩展方法，\n如UIView、UILabel、UIButton、UIColor等的便捷方法，帮助开发者提高开发效率。\n该库还包含了Date、String等基础类型的扩展方法，为iOS开发提供更好的开发体验。",
  "homepage": "https://github.com/1260269149/XXTools_Swift",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "wangxu": "1260269149@qq.com"
  },
  "source": {
    "git": "https://github.com/1260269149/XXTools_Swift.git",
    "tag": "0.1.3"
  },
  "platforms": {
    "ios": "15.0"
  },
  "swift_versions": "5.0",
  "resource_bundles": {
    "XXTools_Swift": [
      "XXTools_Swift/Assets/Localizable/**/*.strings"
    ]
  },
  "frameworks": "UIKit",
  "dependencies": {
    "SnapKit": [
      "~> 5.6.0"
    ],
    "Localize-Swift": [
      "~> 3.2"
    ]
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
    "OTHER_LDFLAGS": "$(inherited)",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
    "LIBRARY_SEARCH_PATHS": "$(inherited)"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited)",
    "FRAMEWORK_SEARCH_PATHS": "$(inherited)",
    "LIBRARY_SEARCH_PATHS": "$(inherited)"
  },
  "source_files": "XXTools_Swift/Classes/**/*.{h,m,swift}",
  "swift_version": "5.0"
}
