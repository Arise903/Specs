{
  "name": "PubmaticAdapter",
  "version": "0.0.100",
  "summary": "an adapter for Pubmatic SDK",
  "description": "an adapter for Pubmatic SDK for MSP C2S",
  "homepage": "https://github.com/aimsp/msp-ios-sdk/MetaAdapter",
  "license": "Copyright",
  "authors": {
    "huanzhiNB": "huanzhi.zhang@newsbreak.com"
  },
  "source": {
    "git": "https://github.com/ParticleMedia/msp-ios-sdk-public.git",
    "tag": "0.0.100"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "PubmaticAdapter/PubmaticAdapter/**/*.{h,m,swift}",
  "exclude_files": "Classes/Exclude",
  "dependencies": {
    "OpenWrapSDK": [
      "4.0.0"
    ],
    "MSPSharedLibraries": [
      "0.0.99"
    ]
  },
  "pod_target_xcconfig": {
    "VALID_ARCHS": "x86_64 armv7 arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "static_framework": true
}
