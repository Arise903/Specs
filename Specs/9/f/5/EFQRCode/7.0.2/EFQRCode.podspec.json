{
  "name": "EFQRCode",
  "version": "7.0.2",
  "summary": "A better way to operate quick response code in Swift.",
  "description": "EFQRCode is a lightweight, pure-Swift library for generating stylized QRCode images with watermark or icon, and for recognizing QRCode from images. Based on `CoreGraphics`, `CoreImage`, and `ImageIO`, EFQRCode provides you a better way to handle QRCode in your app, no matter if it is on iOS, macOS, watchOS, tvOS, and/or visionOS. You can integrate EFQRCode through CocoaPods, Carthage, and/or Swift Package Manager.",
  "homepage": "https://github.com/EFPrefix/EFQRCode",
  "screenshots": "https://raw.githubusercontent.com/EFPrefix/EFQRCode/assets/EFQRCode.jpg",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "EyreFree": "eyrefree@eyrefree.org",
    "ApolloZhu": "public-apollonian@outlook.com"
  },
  "source": {
    "git": "https://github.com/EFPrefix/EFQRCode.git",
    "tag": "7.0.2"
  },
  "social_media_url": "https://twitter.com/EyreFree777",
  "documentation_url": "https://efprefix.github.io/EFQRCode",
  "platforms": {
    "ios": "13.0",
    "tvos": "13.0",
    "osx": "10.15",
    "watchos": "6.0",
    "visionos": "1.0"
  },
  "swift_versions": "5.0",
  "requires_arc": true,
  "frameworks": [
    "ImageIO",
    "CoreGraphics",
    "Foundation"
  ],
  "ios": {
    "frameworks": "CoreImage"
  },
  "tvos": {
    "frameworks": "CoreImage"
  },
  "osx": {
    "frameworks": "CoreImage"
  },
  "visionos": {
    "frameworks": "CoreImage"
  },
  "dependencies": {
    "SwiftDraw": [
      "~> 0.22.0"
    ],
    "ef_swift_qrcodejs": [
      "~> 2.3.1"
    ]
  },
  "source_files": "Source/**/*.{h,swift}",
  "swift_version": "5.0"
}
