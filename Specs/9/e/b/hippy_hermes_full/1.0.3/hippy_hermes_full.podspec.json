{
  "name": "hippy_hermes_full",
  "version": "1.0.3",
  "summary": "hermes engine for hippy",
  "description": "version of hermes 0.76",
  "homepage": "https://github.com/hippy-contrib/hippy-hermes-bin",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Author": "author@example.com"
  },
  "source": {
    "git": "https://github.com/hippy-contrib/hippy-hermes-bin.git",
    "tag": "1.0.3"
  },
  "platforms": {
    "ios": "12.0"
  },
  "source_files": "Full/destroot/**/*.{h,m,swift}",
  "public_header_files": "Full/destroot/include/**/*.h",
  "header_mappings_dir": "Full/destroot/include",
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++17",
    "HEADER_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/Full/destroot/include",
    "GCC_ENABLE_CPP_EXCEPTIONS": true,
    "GCC_ENABLE_CPP_RTTI": true
  },
  "ios": {
    "vendored_frameworks": "Full/destroot/Library/Frameworks/universal/hermes.xcframework"
  }
}
