{
  "name": "nimbbl_mobile_kit_ios_webview_sdk",
  "version": "1.0.4",
  "summary": "Nimbbl WebView SDK for iOS",
  "description": "WebView SDK for Nimbbl payment integration on iOS. This SDK provides a WebView-based checkout experience for payment processing.",
  "homepage": "https://nimbbl.biz",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Nimbbl": "tools@nimbbl.biz"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/nimbbl-tech/nimbbl_mobile_kit_ios_webview_pod.git",
    "tag": "1.0.4"
  },
  "static_framework": true,
  "source_files": "static_lib/Headers/*.h",
  "pod_target_xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(PODS_ROOT)/nimbbl_mobile_kit_ios_webview_sdk/static_lib",
    "OTHER_LDFLAGS": "-lnimbbl_mobile_kit_ios_webview_sdk",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_versions": "5.0",
  "frameworks": "WebKit",
  "dependencies": {
    "nimbbl_mobile_kit_ios_core_api_sdk": [
      "1.0.7"
    ]
  },
  "swift_version": "5.0"
}
