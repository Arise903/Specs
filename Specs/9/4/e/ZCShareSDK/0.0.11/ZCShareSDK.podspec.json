{
  "name": "ZCShareSDK",
  "version": "0.0.11",
  "summary": "ZCShareSDK的描述",
  "homepage": "https://gitee.com/yzc1991/zcshare-sdk",
  "license": "MIT",
  "authors": {
    "yzc": "773825166@qq.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://gitee.com/yzc1991/zcshare-sdk.git",
    "tag": "0.0.11"
  },
  "source_files": "ZCShareSDK/**/*",
  "exclude_files": [
    "ZCShareSDK/**/*/*.png",
    "ZCShareSDK/**/*/Info.plist",
    "ZCShareSDK/**/*.bundle"
  ],
  "frameworks": [
    "SystemConfiguration",
    "ImageIO",
    "Photos",
    "CoreTelephony"
  ],
  "libraries": [
    "c++",
    "sqlite3",
    "z"
  ],
  "pod_target_xcconfig": {
    "OTHER_LINK_FLAG": "$(inherited)-ObjC",
    "VALID_ARCHS[sdk=iphone*]": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "VALID_ARCHS[sdk=iphone*]": "arm64",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
