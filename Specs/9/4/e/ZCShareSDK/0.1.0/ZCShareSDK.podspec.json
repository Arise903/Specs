{
  "name": "ZCShareSDK",
  "version": "0.1.0",
  "summary": "ZCShareSDK的描述",
  "homepage": "https://gitee.com/yzc1991/zcshare-sdk",
  "license": "MIT",
  "authors": {
    "yzc": "773825166@qq.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://gitee.com/yzc1991/zcshare-sdk.git",
    "tag": "0.1.0"
  },
  "source_files": [
    "ZCShareSDK/*.{h,m}",
    "ZCShareSDK/SDK/QQ/*.h",
    "ZCShareSDK/SDK/Sina/*.h",
    "ZCShareSDK/SDK/Sina/SinaSDK/*.h",
    "ZCShareSDK/SDK/WeChat/*.h",
    "ZCShareSDK/SDK/WeChat/WechatSDK/*.h"
  ],
  "resources": "ZCShareSDK/**/*.bundle",
  "frameworks": [
    "SystemConfiguration",
    "ImageIO",
    "Photos",
    "CoreTelephony"
  ],
  "libraries": [
    "c++",
    "sqlite3",
    "z"
  ],
  "vendored_frameworks": [
    "ZCShareSDK/**/*.framework",
    "ZCShareSDK/**/*.xcframework"
  ],
  "vendored_libraries": "ZCShareSDK/**/*.a",
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "$(inherited) -ObjC",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  }
}
