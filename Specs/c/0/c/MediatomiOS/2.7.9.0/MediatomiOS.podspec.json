{
  "name": "MediatomiOS",
  "version": "2.7.9.0",
  "summary": "Mobile App Aggregation Ad SDK of Mediatom SDK for iOS.",
  "description": "Mediatom SDK is an advertising aggregation SDK that allows you to monetize iOS applications through Mediatom advertising.",
  "homepage": "http://gitlab.ydmob.com/ios/PublicSDK/mediatom/mediatom",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
