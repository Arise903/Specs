  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "i"
  },
  "source": {
    "git": "http://gitlab.ydmob.com/ios/PublicSDK/mediatom/mediatom.git",
