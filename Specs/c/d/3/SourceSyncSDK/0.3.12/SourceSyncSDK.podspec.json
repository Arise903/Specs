{
  "name": "SourceSyncSDK",
  "version": "0.3.12",
  "summary": "A framework for handling activation details in iOS apps.",
  "description": "SourceSyncSDK provides UI components for activation templates, including headers, previews, and detailed views. This SDK helps developers integrate Source Digital's platform features into their iOS applications.",
  "homepage": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Source Digital": "dev@sourcedigital.net"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios.git",
    "tag": "0.3.12"
  },
  "source_files": "SourceSyncSDK/Sources/**/*.{h,m,swift}",
  "exclude_files": [
    "SourceSyncSDK/**/Package.swift",
    "Example/**/*",
    "Tests/**/*"
  ],
  "dependencies": {
    "DivKit": [
      "~> 31.14.0"
    ],
    "DivKitExtensions": [
      "~> 31.14.0"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "x86_64",
    "VALID_ARCHS": "arm64 arm64e",
    "SUPPORTS_MACCATALYST": "NO"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "x86_64"
  },
  "ios": {
    "frameworks": "UIKit",
    "pod_target_xcconfig": {
      "ONLY_ACTIVE_ARCH": "NO"
    }
  },
  "swift_versions": "5.7",
  "requires_arc": true,
  "module_name": "SourceSyncSDK",
  "swift_version": "5.7"
}
