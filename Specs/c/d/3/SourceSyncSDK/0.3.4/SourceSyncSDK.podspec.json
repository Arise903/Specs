{
  "name": "SourceSyncSDK",
  "version": "0.3.4",
  "summary": "A framework for handling activation details in iOS and tvOS apps.",
  "description": "SourceSyncSDK provides UI components for activation templates, including headers, previews, and detailed views. This SDK helps developers integrate Source Digital's platform features into their iOS and tvOS applications.",
  "homepage": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Source Digital": "dev@sourcedigital.net"
  },
  "platforms": {
    "ios": "13.0",
    "tvos": "13.0"
  },
  "source": {
    "git": "https://github.com/Source-Digital/sourcesync-sdk-ui-ios.git",
    "tag": "0.3.4"
  },
  "source_files": "SourceSyncSDK/Sources/**/*.{h,m,swift}",
  "exclude_files": [
    "SourceSyncSDK/**/Package.swift",
    "Example/**/*",
    "Tests/**/*"
  ],
  "pod_target_xcconfig": {
    "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) TVOS_BUILD"
  },
  "ios": {
    "frameworks": "UIKit"
  },
  "tvos": {
    "frameworks": [
      "UIKit",
      "TVUIKit"
    ]
  },
  "swift_versions": "5.7",
  "requires_arc": true,
  "swift_version": "5.7"
}
