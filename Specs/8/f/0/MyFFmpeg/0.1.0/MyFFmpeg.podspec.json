  },
  "source": {
    "git": "https://github.com/pethanihiten/MyFFmpeg.git",
    "tag": "0.1.0"
  },
  "summary": "MyFFmpeg is a Swift wrapper for FFmpeg that enables easy video and audio processing within iOS",
  "social_media_url": "https://twitter.com/pethanihiten",
  "description": "SoundWave is a customizable view representing sounds over time:\n- Add and display audio metering level values on the fly\n- Set array of pre-existing audio metering level and play / pause / resume\n- Customize background, gradient start and end colors, metering level bar properties, etc.",
  "platforms": {
    "ios": "12.4"
  },
  "source_files": [
    "MyFFmpeg/*.swift",
    "MyFFmpeg/Classes/**/*.swift"
  ],
  "vendored_frameworks": "Frameworks/*.framework",
  "xcconfig": {
    "OTHER_LDFLAGS": "-ObjC -framework AVFoundation -framework AudioToolbox -framework CoreMedia -framework VideoToolbox -lbz2 -lc++ -lz"
  },
  "libraries": [
    "bz2",
    "c++",
    "z"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5"
}
