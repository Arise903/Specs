{
  "name": "ReBid",
  "module_name": "ReBidSdk",
  "version": "5.0.3",
  "summary": "This is the iOS SDK of ReBid.",
  "platforms": {
    "ios": "12.0"
  },
  "frameworks": "SystemConfiguration",
  "ios": {
    "weak_frameworks": "AdSupport"
  },
  "tvos": {
    "weak_frameworks": "AdSupport"
  },
  "default_subspecs": "ReBid",
  "license": {
    "type": "Proprietary"
  },
  "authors": {
    "Engineering ReBid": "engineering@rebid.co"
  },
  "homepage": "https://bitbucket.org/rebid_product/reb-ios-sdk",
  "source": {
    "git": "git@bitbucket.org:rebid_product/reb-ios-sdk.git",
    "tag": "5.0.2"
  },
  "subspecs": [
    {
      "name": "ReBid",
      "source_files": [
        "ReBid/**/*.{h,m}",
        "UmbrellaHeaders/sdk/*.{h,m}"
      ],
      "public_header_files": [
        "ReBid/*.h",
        "UmbrellaHeaders/sdk/*.h"
      ],
      "exclude_files": "ReBid/include/**/*.h",
      "resource_bundles": {
        "ReBid": [
          "ReBid/*.xcprivacy"
        ]
      },
      "header_dir": "ReBidSdk"
    },
    {
      "name": "ReBidWebBridge",
      "source_files": [
        "ReBid/**/*.{h,m}",
        "ReBidBridge/*.{h,m}",
        "UmbrellaHeaders/webbridge/*.{h,m}"
      ],
      "public_header_files": [
        "ReBid/*.h",
        "ReBidBridge/*.h",
        "UmbrellaHeaders/webbridge/*.h"
      ],
      "exclude_files": [
        "ReBid/include/**/*.h",
        "ReBidBridge/include/**/*.h"
      ],
      "resource_bundles": {
        "ReBid": [
          "ReBid/*.xcprivacy"
        ]
      },
      "header_dir": "ReBidSdk",
      "platforms": {
        "ios": "12.0"
      }
    }
  ]
}
