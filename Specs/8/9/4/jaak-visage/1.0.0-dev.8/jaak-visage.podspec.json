{
  "name": "jaak-visage",
  "version": "1.0.0-dev.8",
  "summary": "AI-powered face detection and recording library for iOS using MediaPipe BlazeFace",
  "description": "JAAKVisage is an advanced face detection library that provides:\n- Real-time face detection using MediaPipe BlazeFace\n- Auto-recording with quality analysis\n- Face quality metrics and validation\n- Security monitoring and device validation\n- Customizable UI overlays and instructions\n- Video recording with real-time face analysis\n- Comprehensive error handling with recovery",
  "homepage": "https://jaak.ai",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Diego Bruno": "diego.bruno@jaak.ai"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "http": "https://storage.googleapis.com/jaak-ios-prerelease/jaak-visage/1.0.0-dev.8/JAAKVisage-1.0.0-dev.8.zip",
    "sha256": "97ff3d74e27febae5f6d861ec3517e4a376f7885a5b1457dff1a06078466ef20"
  },
  "vendored_frameworks": "JAAKVisage.xcframework",
  "resources": [
    "Resources/Models/blaze_face_short_range.tflite",
    "Resources/icon-cap-dark.png",
    "Resources/icon-center-face-dark.png",
    "Resources/icon-glasses-dark.png",
    "Resources/icon-headphones-dark.png"
  ],
  "frameworks": [
    "UIKit",
    "AVFoundation",
    "Vision",
    "CoreML",
    "Foundation",
    "OpenGLES",
    "Metal"
  ],
  "dependencies": {
    "MediaPipeTasksVision": [
      "~> 0.10.3"
    ]
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "5.0",
