  },
  "authors": {
    "Diego Bruno": "diego.bruno@jaak.ai"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "http": "https://storage.googleapis.com/jaak-ios-prerelease/jaak-visage/1.0.0-beta.2/JAAKVisage-1.0.0-beta.2.zip",
