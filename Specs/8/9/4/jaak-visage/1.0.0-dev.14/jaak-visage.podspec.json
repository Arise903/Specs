  "license": {
    "type": "MIT"
  },
  "authors": {
    "Diego Bruno": "diego.bruno@jaak.ai"
  },
  "platforms": {
    "ios": "12.0"
  },
  "swift_versions": "5.0",
  "source": {
    "http": "https://storage.googleapis.com/jaak-ios-prerelease/jaak-visage/1.0.0-dev.14/JAAKVisage-1.0.0-dev.14.zip",
    "sha256": "2851d52b1768a50b2eb6564857d784f925346c2bfbf80d562a4aee783d71b3ce"
  },
  "vendored_frameworks": "JAAKVisage.xcframework",
  "resources": [
    "Resources/Models/blaze_face_short_range.tflite",
    "Resources/icon-cap-dark.png",
    "Resources/icon-center-face-dark.png",
    "Resources/icon-glasses-dark.png",
    "Resources/icon-headphones-dark.png"
  ],
